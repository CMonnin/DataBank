{
  "name": "@databank/api",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "license": "AGPL-3.0",
  "scripts": {
    "build": "node scripts/build.js",
    "dev": "NODE_ENV=development env-cmd -f ../.env node --max-old-space-size=4096 scripts/dev.js",
    "build:watch": "node scripts/build.js --watch",
    "db:generate": "prisma generate",
    "format": "prettier --write src",
    "lint": "tsc && eslint --fix src",
    "test": "env-cmd -f ../.env vitest",
    "test:coverage": "env-cmd -f ../.env vitest --coverage",
    "test:e2e": "vitest test/"
  },
  "dependencies": {
    "@douglasneuroinformatics/libjs": "^0.0.3",
    "@douglasneuroinformatics/libnest": "^0.0.2",
    "@nestjs/common": "^10.3.10",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.3.10",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-express": "^10.3.10",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/throttler": "^5.2.0",
    "@prisma/client": "^5.16.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "express": "^4.19.2",
    "multer": "1.4.5-lts.1",
    "nodejs-polars": "^0.13.0",
    "nodemailer": "^6.9.14",
    "reflect-metadata": "~0.1.14",
    "rxjs": "^7.8.1",
    "type-fest": "^4.21.0",
    "udsv": "^0.5.3",
    "zod": "^3.23.8"
