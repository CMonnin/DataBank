{
  "name": "databank",
  "private": true,
  "type": "module",
  "workspaces": [
    "api",
    "web",
    "types"
  ],
  "scripts": {
    "build": "env-cmd -f .env.prod turbo run build",
    "clean": "fd -H -I --prune 'dist|node_modules' --exec rm -rf",
    "deploy": "./scripts/deploy.sh",
    "dev": "env-cmd -f .env.dev turbo run dev",
    "lint": "turbo run lint",
    "test": "env-cmd -f .env.dev turbo run test"
  },
  "dependencies": {
    "ts-pattern": "^5.0.5",
    "type-fest": "^4.2.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@databank/types": "*",
    "@douglasneuroinformatics/eslint-config": "^1.1.0",
    "@types/node": "^20.5.6",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "eslint": "^8.48.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "globals": "^13.21.0",
    "prettier": "^3.0.2",
    "turbo": "^1.10.13",
    "typescript": "^5.2.2"
  },
  "packageManager": "npm@9.6.7",
  "engines": {
    "node": ">=20.0.0 <21.0.0",
    "npm": ">=9.0.0 <10.0.0"
  }
}
