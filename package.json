{
  "name": "databank",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@8.15.3",
  "scripts": {
    "build": "env-cmd turbo run build",
    "clean": "find . -type d '(' -name '.turbo' -o -name 'dist' -o -name 'node_modules' ')' -prune -exec rm -rf {} \\;",
    "db:generate": "turbo run db:generate",
    "dev": "env-cmd turbo run dev",
    "format": "turbo run format",
    "lint": "turbo run lint",
    "prepare": "husky install",
    "test": "env-cmd turbo run test"
  },
  "devDependencies": {
    "@databank/types": "workspace:*",
    "@douglasneuroinformatics/eslint-config": "^4.1.1",
    "@types/node": "^20.11.25",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "turbo": "^1.12.5",
    "typescript": "~5.3.3",
    "vitest": "^1.3.1"
  }
}
